{
  "compilerOptions": {
    "jsx": "react-native",
    "checkJs": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "target": "es2020",
    "lib":[
      "ES2015"
    ],
    "module": "es2015",
    "baseUrl": "src"
  },
  "exclude": [
    "dist",
    "node_modules",
    "build",
    ".vscode",
    "coverage",
    ".npm",
    ".yarn"
  ],
  "typeAcquisition": {
    "enable": true,
    "include": [
      "react",
      "react-native",
      "react-dom",
      "jest",
      "testing-library__jest-dom"
    ]
  }
}
